{
  "expo": {
    "name": "NewsAinchor",
    "slug": "NewsAinchor",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/News-AInchor-Logo-Blue.png",
      "resizeMode": "cover",
      "backgroundColor": "#2d5286"
    },
    "icon": "./assets/images/News-AInchor-Logo-Blue.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.NewsAinchor"
    },
    "android": {
      "package": "com.anonymous.NewsAinchor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/News-AInchor-Logo-Blue.png",
        "backgroundColor": "#2d5286"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
